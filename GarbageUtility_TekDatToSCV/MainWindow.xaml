<Window x:Class="Pavlo.MyDAL.GarbageUtility_TekDatToSCV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:Pavlo.MyDAL.GarbageUtility_TekDatToSCV"
        mc:Ignorable="d"
        Title="View frame from *.dat file" Height="450" Width="800">
    <Window.DataContext>
        <local:MainWindowViewmodel/>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Model="{Binding Path=TheModel}">
        </oxy:PlotView>
    </Grid>
</Window>
